<!DOCTYPE html>

<title>User</title>

{% block body %}
    <h1>User</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ user.id }}</td>
            </tr>
            <tr>
                <th>Email</th>
                <td>{{ user.email }}</td>
            </tr>
            <tr>
                <th>Roles</th>
                <td>{{ user.roles ? user.roles|json_encode : '' }}</td>
            </tr>
            <tr>
                <th>Password</th>
                <td>{{ user.password }}</td>
            </tr>
            <tr>
                <th>IsVerified</th>
                <td>{{ user.isVerified ? 'Yes' : 'No' }}</td>
            </tr>
            <tr>
                <th>Image</th>
                <td>{{ user.image }}</td>
            </tr>
            <tr>
                <th>Speciality</th>
                <td>{{ user.Speciality }}</td>
            </tr>
            <tr>
                <th>Licence</th>
                <td>{{ user.Licence }}</td>
            </tr>
            <tr>
                <th>Location</th>
                <td>{{ user.location }}</td>
            </tr>
            <tr>
                <th>PhoneNumber</th>
                <td>{{ user.phoneNumber }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_user_info_index') }}">back to list</a>

    <a href="{{ path('app_user_info_edit', {'id': user.id}) }}">edit</a>

    {{ include('user_info/_delete_form.html.twig') }}
{% endblock %}
