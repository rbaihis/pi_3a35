{% extends 'base_front_event.html.twig' %}

{% block title %}EventTicket{% endblock %}

{% block body %}
    <h1>EventTicket</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ event_ticket.id }}</td>
            </tr>
            <tr>
                <th>Matricule_event</th>
                <td>{{ event_ticket.matriculeEvent }}</td>
            </tr>
            <tr>
                <th>Image</th>
                <td>{{ event_ticket.image }}</td>
            </tr>
            <tr>
                <th>Date_ticket</th>
                <td>{{ event_ticket.dateTicket ? event_ticket.dateTicket|date('Y-m-d') : '' }}</td>
            </tr>
            <tr>
                <th>Valide_ticket</th>
                <td>{{ event_ticket.valideTicket }}</td>
            </tr>
            <tr>
                <th>Prix_ticket</th>
                <td>{{ event_ticket.prixTicket }}</td>
            </tr>
            <tr>
                <th>Description_ticket</th>
                <td>{{ event_ticket.descriptionTicket }}</td>
            </tr>
            <tr>
                <th>Adresse</th>
                <td>{{ event_ticket.adresse }}</td>
            </tr>
            <tr>
                <th>Reserve</th>
                <td>{{ event_ticket.reserve }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_events_ticket_index') }}">back to list</a>

    <a href="{{ path('app_events_ticket_edit', {'id': event_ticket.id}) }}">edit</a>

    {{ include('events_ticket/_delete_form.html.twig') }}
{% endblock %}
